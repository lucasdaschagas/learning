function(t,e,i){var o=/\+/g;function n(t){return t}function a(t){return decodeURIComponent(t.replace(o," "))}var s=t.cookie=function(i,o,r){if(void 0!==o){if(r=t.extend({},s.defaults,r),null===o&&(r.expires=-1),"number"==typeof r.expires){var l=r.expires,h=r.expires=new Date;h.setDate(h.getDate()+l)}return o=s.json?JSON.stringify(o):String(o),e.cookie=[encodeURIComponent(i),"=",s.raw?o:encodeURIComponent(o),r.expires?"; expires="+r.expires.toUTCString():"",r.path?"; path="+r.path:"",r.domain?"; domain="+r.domain:"",r.secure?"; secure":""].join("")}for(var c=s.raw?n:a,u=e.cookie.split("; "),d=0,p=u.length;d<p;d++){var m=u[d].split("=");if(c(m.shift())===i){var f=c(m.join("="));return s.json?JSON.parse(f):f}}return null};s.defaults={},t.removeCookie=function(e,i){return null!==t.cookie(e)&&(t.cookie(e,null,i),!0)}}(jQuery,document);