    /**
     * Specificity based symbol resolver
     */
    s7ve.SymbolTree = (function () {

        /**
         * SymbolNode
         */
        function SymbolNode(parent) {
            this.parent = parent;
            this.value = "";
            this.children = {};
        }
        /**
         * Create and add child node for key, or return existing one.
         * @param key {String}
         * @returns {SymbolNode}
         */
        SymbolNode.prototype.addChild = function (key) {
            return this.children[key] = (this.children[key] || new SymbolNode(this));
        };
        /**
         * Resolve symbol against SymbolNode tree
         * @param symbol
         * @returns {String} resolved symbol text
         */
        SymbolNode.prototype.resolve = function (symbol) {

            var tokens = symbol.split("_"),
                node = this.children[tokens.pop()];

            // The target symbol token must be present at root
            if (!node) {
                return symbol;
            }

            node = tokens.reverse().reduce(function (prev, curr) {
                return prev.children[curr] || prev;
            }, node);

            return node.getValue() || symbol;
        };
        /**
         * Return nearest truthy value from self or ancestor node
         * @returns {String}
         */
        SymbolNode.prototype.getValue = function () {
            return this.value || (this.parent && this.parent.getValue());
        };

        var result = new SymbolNode();

        // Process s7ve.SymbolText map into tree of SymbolNode objects
        Object.keys(s7ve.SymbolText).forEach(function (symbol) {
            var tokens = symbol.split("_"),
                currentLevel = result;

            tokens.reverse().forEach(function (token, index) {
                currentLevel = currentLevel.addChild(token);
                if (index === tokens.length - 1) {
                    // leaf node, set resolved value
                    currentLevel.value = s7ve.SymbolText[symbol];
                }
            });
        });

        // Processed SymbolNode tree
        return result;

    }());

}(window.s7ve, window.Granite));